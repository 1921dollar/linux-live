[begin] (Desktop menu)

   [exec] (Terminal) { fbstartupnotify && xterm -ls }
   [exec] (Web browser) { fbstartupnotify && chromium }
   [exec] (Text editor) { fbstartupnotify && leafpad }
   [exec] (Calculator) { fbstartupnotify && qalculate }
   [separator]
   [workspaces] (Workspaces)
   [exec] (Exit) { fblogout }

[end]
